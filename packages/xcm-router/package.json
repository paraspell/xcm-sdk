{
  "name": "@paraspell/xcm-router",
  "version": "1.2.2",
  "description": "Tool for XCM cross-chain asset exchanging across Polkadot and Kusama ecosystems",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "rollup -c",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "test:integration": "vitest run --config ./vitest.config.integration.ts",
    "runAll": "yarn compile && yarn format:write && yarn lint && yarn test",
    "updateAssets": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/updateAssets.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@acala-network/api": "^6.1.3",
    "@acala-network/app-util": "^4.1.5",
    "@acala-network/eth-providers": "^2.8.7",
    "@acala-network/sdk": "4.1.9-13",
    "@acala-network/sdk-core": "4.1.9-13",
    "@acala-network/sdk-swap": "4.1.9-13",
    "@acala-network/sdk-wallet": "4.1.8-14",
    "@crypto-dex-sdk/amm": "^0.1.2",
    "@crypto-dex-sdk/chain": "^0.1.2",
    "@crypto-dex-sdk/currency": "^0.1.2",
    "@crypto-dex-sdk/format": "^0.1.2",
    "@crypto-dex-sdk/math": "^0.1.2",
    "@crypto-dex-sdk/parachains-bifrost": "^0.1.2",
    "@crypto-dex-sdk/token-lists": "^0.1.2",
    "@galacticcouncil/sdk": "^4.0.2",
    "@paraspell/sdk": "workspace:*",
    "@polkadot/api": "^12.2.2",
    "@polkadot/api-base": "^12.2.2",
    "@polkadot/apps-config": "^0.142.1",
    "@polkadot/types": "^12.2.2",
    "@polkadot/types-codec": "^12.2.2",
    "@polkadot/util": "^13.0.2",
    "@zenlink-types/bifrost": "^1.7.6",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "ethers": "^6.13.2",
    "inter-exchange": "^2.0.0",
    "jsbi": "^4.3.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.40",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.3.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-syntax-import-assertions": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.12",
    "@vitest/coverage-v8": "^2.0.5",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3"
  }
}
